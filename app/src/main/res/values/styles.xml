    <style name="RT.AppTheme.Battery" parent="RT.AppTheme.Dark" />

    <style name="RT.AppTheme.Dark" parent="Base.RT.AppTheme.Dark" />

    <style name="RT.AppTheme.Black" parent="Base.RT.AppTheme.Black" />

    <style name="RT.AppTheme.DayNight" parent="RT.AppTheme.Light" />

    <style name="RT.AppTheme.Launch" parent="Theme.AppCompat.Light" />

    <style name="RT.AppTheme.Light" parent="Base.RT.AppTheme.Light" />

    <style name="RT.AppTheme.System" parent="RT.AppTheme.Dark" />

    <style name="RT.AppTheme.BatteryBlack" parent="RT.AppTheme.Black" />

    <style name="RT.AppTheme.DayNightBlack" parent="RT.AppTheme.Light" />

    <style name="RT.AppTheme.SystemBlack" parent="RT.AppTheme.Black" />

    <style name="RT.PreferenceTheme" parent="PreferenceThemeOverlay.v14" />

    <style name="RT.Widget.ButtonBar.TextButton" parent="Widget.Material3.Button.TextButton.Dialog">
        <item name="android:letterSpacing" tools:ignore="NewApi">0.0</item>
        <item name="android:textColor">?colorSecondary</item>
        <item name="rippleColor">?colorSecondaryVariant</item>
    </style>

    <style name="RT.Widget.ButtonBar.TextButton.Neutral" parent="Widget.Material3.Button.TextButton.Dialog.Flush">
        <item name="android:letterSpacing" tools:ignore="NewApi">0.0</item>
        <item name="android:textColor">?colorSecondary</item>
        <item name="rippleColor">?colorSecondaryVariant</item>
    </style>

    <style name="RT.Widget.ScrollView.TopIndicator" parent="@android:style/Widget.ScrollView" />

    <style name="RT.Widget.Snackbar" parent="Widget.Material3.Snackbar">
        <item name="actionTextColorAlpha">1.0</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="backgroundTint">?colorSurfaceVariant</item>
    </style>

    <style name="RT.Widget.Snackbar.TextButton" parent="Widget.Material3.Button.TextButton.Snackbar">
        <item name="android:textColor">?colorSecondary</item>
        <item name="android:letterSpacing" tools:ignore="NewApi">0.0</item>
        <item name="rippleColor">?colorSecondaryVariant</item>
    </style>

    <style name="RT.Widget.Toolbar" parent="Widget.Material3.Toolbar.Surface">
    </style>

    <style name="RT.View.LinkImageButton" parent="Widget.AppCompat.ImageButton">
        <item name="android:background">?selectableItemBackgroundBorderless</item>
        <item name="android:layout_height">@dimen/author_info_link_image_size</item>
        <item name="android:layout_width">@dimen/author_info_link_image_size</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="RT.View.Changelog" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">?colorOnSurface</item>
        <item name="android:background">?changelogBackground</item>
        <item name="android:padding">@dimen/changelog_padding</item>
    </style>

    <style name="Base.RT.Design.BottomSheetDialog" parent="ThemeOverlay.Material3.BottomSheetDialog">

        <item name="bottomSheetStyle">@style/Widget.RT.BottomSheet</item>
    </style>

    <style name="RT.Design.BottomSheetDialog" parent="Base.RT.Design.BottomSheetDialog" />

    <style name="Widget.RT.BottomSheet" parent="Widget.Material3.BottomSheet">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.RT.BottomSheet</item>
        <item name="android:elevation" tools:ignore="NewApi,PrivateResource">@dimen/m3_sys_elevation_level0</item>
    </style>

    <style name="ShapeAppearanceOverlay.RT.BottomSheet" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">@dimen/bottom_sheet_corner_radius</item>
        <item name="cornerSizeTopRight">@dimen/bottom_sheet_corner_radius</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

</resources>
